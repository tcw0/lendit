{
  "name": "lendit-client",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "prepare": "cd .. && husky install",
    "dev": "yarn api:gen && vite",
    "build": "yarn api:gen && yarn clean && tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "start": "vite preview",
    "clean": "rimraf dist",
    "api:gen": "rimraf ../api/generated && openapi --input ../api/src/api.yaml --output ../api/generated --request ../api/src/request.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.1",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.2",
    "@mui/system": "^5.13.7",
    "@reach/combobox": "^0.18.0",
    "@react-spring/web": "9.5.5",
    "@types/react-swipeable-views": "^0.13.2",
    "@stripe/react-stripe-js": "^2.1.1",
    "@stripe/stripe-js": "^1.54.1",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.9",
    "geolib": "^3.3.4",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "mapbox-gl": "^2.15.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-touch-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-google-autocomplete": "^2.7.3",
    "react-map-gl": "^7.1.2",
    "react-router-dom": "^6.11.1",
    "react-swipeable-views": "^0.14.0",
    "react-tinder-card": "^1.6.2",
    "rsuite": "^5.37.0",
    "use-places-autocomplete": "^4.0.0"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^20.3.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-css": "^0.7.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "^8.0.0",
    "openapi-typescript-codegen": "^0.24.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "typescript": "*",
    "vite": "^4.3.2"
  }
}
